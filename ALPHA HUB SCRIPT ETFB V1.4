local function RemoveESP(block)
    if block:FindFirstChild("ESPHighlight") then block.ESPHighlight:Destroy() end
    if block:FindFirstChild("ESPLabel") then block.ESPLabel:Destroy() end
end

local function ApplyLB_ESP(block)
    RemoveESP(block)
    if not _G.LB_ESP_Enabled then return end

    -- Detect Rarity, Mutation, and Name
    local rarityObj = block:FindFirstChild("Rarity") or block:FindFirstChild("Type")
    local mutationObj = block:FindFirstChild("Mutation")
    
    local rarityName = rarityObj and tostring(rarityObj.Value) or block.Name
    local mutationName = mutationObj and tostring(mutationObj.Value) or ""
    
    -- Combine display name
    local displayName = mutationName ~= "" and (mutationName .. " " .. rarityName) or rarityName
    
    -- Special Check for Rare Blocks
    local nameLower = string.lower(block.Name)
    local rarityLower = string.lower(rarityName)
    
    local isUfo = string.find(nameLower, "ufo") or string.find(rarityLower, "ufo")
    local isAdmin = string.find(nameLower, "admin") or string.find(rarityLower, "admin")
    local isRadioactive = string.find(nameLower, "radioactive") or string.find(rarityLower, "radioactive")
    
    if isUfo then 
        displayName = "ðŸ›¸ UFO LUCKY BLOCK" 
    elseif isAdmin then
        displayName = "âš¡ ADMIN LUCKY BLOCK"
    elseif isRadioactive then
        displayName = "â˜¢ï¸ RADIOACTIVE LUCKY BLOCK"
    end

    local shouldShow = false
    if table.find(_G.LB_Filters, "All") then
        shouldShow = true
    else
        -- Check if current block matches any selected filters
        for _, filterValue in ipairs(_G.LB_Filters) do
            if string.find(string.lower(displayName), string.lower(filterValue)) then
                shouldShow = true
                break
            end
        end
    end

    if not shouldShow then return end

    -- Visual Highlight
    local hl = Instance.new("Highlight")
    hl.Name = "ESPHighlight"
    
    -- Color Logic
    if isAdmin then
        hl.FillColor = Color3.fromRGB(255, 50, 50) -- Bright Red
    elseif isRadioactive then
        hl.FillColor = Color3.fromRGB(150, 255, 0) -- Lime Green
    elseif isUfo then
        hl.FillColor = Color3.fromRGB(0, 255, 100) -- Neon Cyan/Green
    elseif mutationName ~= "" then
        hl.FillColor = Color3.fromRGB(255, 0, 255) -- Purple for Mutated
    else
        hl.FillColor = Color3.fromRGB(255, 215, 0) -- Gold for Normal
    end
    
    hl.OutlineColor = Color3.new(1,1,1)
    hl.Parent = block

    -- Billboard Label
    local bb = Instance.new("BillboardGui")
    bb.Name = "ESPLabel"
    bb.Size = UDim2.new(0, 200, 0, 50)
    bb.StudsOffset = Vector3.new(0, 4, 0)
    bb.AlwaysOnTop = true
    
    local lbl = Instance.new("TextLabel")
    lbl.Parent = bb
    lbl.BackgroundTransparency = 1
    lbl.Size = UDim2.new(1,0,1,0)
    lbl.Text = displayName
    
    -- Label Color Logic
    if isAdmin then lbl.TextColor3 = Color3.fromRGB(255, 0, 0)
    elseif isRadioactive then lbl.TextColor3 = Color3.fromRGB(170, 255, 0)
    elseif isUfo then lbl.TextColor3 = Color3.fromRGB(0, 255, 150)
    else lbl.TextColor3 = Color3.new(1,1,1) end
    
    lbl.TextStrokeTransparency = 0
    lbl.TextScaled = true
    
    bb.Parent = block
end

local function RefreshESP()
